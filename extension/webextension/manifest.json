{

  "description": "UI for interacting with Tracking Protection.",
  "manifest_version": 2,
  "name": "tracking-protection-ui",
  "version": "1.0",
  "homepage_url": "https://github.com/rhelmer/tracking-protection-study",

  "background": {
    "scripts": ["background.js"]
  },

  "page_action": {
    "default_popup": "popup.html",
    "browser_style": true
  },
  
  "permissions": [
    "activeTab",
    "tabs"
  ]

}
